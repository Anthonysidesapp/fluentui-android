<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke
                android:width="1dp"
                android:color="?attr/FluentuiButtonOutlinedStrokeDisabledColor" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="1dp"
                android:color="?attr/FluentuiButtonOutlinedStrokePressedColor" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="?attr/fluentuiButtonOutlinedStrokeDefaultColor" />
        </shape>
    </item>
</selector>